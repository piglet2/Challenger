package com.envision.envservice.entity.bo;

import java.util.List;



/**
 * @ClassName AssessmentInfoBo
 * @author caisong
 * @date 2016-008-23
 * 被评价人收到的评价列表
 * 
 */
public class AssessmentInfoBo {

	private Integer id;
	
	/*
	 * 周期id
	*/
	private String cycleId;
	
	/*
	 * 被评论个人id
	*/
	private String userId;
	
	/*
	 * 意志平均分
	*/
	private String priseWillAverage;
	
	/*
	 * 智慧平均分
	*/
	private String priseWisdomAverage;
	
	/*
	 * 博爱平均分
	*/
	private String priseLoveAverage;
	
	/*
	 * 开始周期
	*/
	private String periodDateFrom;
	
	/*
	 * 结束周期
	*/
	private String periodDateTo;
	
	/*
	 * 描述第几期
	*/
	private String des;

	/*
	 * 评论详情
	 * 
	 * */
	private List<AssessmentBo> AsList;

	public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public String getCycleId() {
		return cycleId;
	}

	public void setCycleId(String cycleId) {
		this.cycleId = cycleId;
	}

	public String getUserId() {
		return userId;
	}

	public void setUserId(String userId) {
		this.userId = userId;
	}

	public String getPriseWillAverage() {
		return priseWillAverage;
	}

	public void setPriseWillAverage(String priseWillAverage) {
		this.priseWillAverage = priseWillAverage;
	}

	public String getPriseWisdomAverage() {
		return priseWisdomAverage;
	}

	public void setPriseWisdomAverage(String priseWisdomAverage) {
		this.priseWisdomAverage = priseWisdomAverage;
	}

	public String getPriseLoveAverage() {
		return priseLoveAverage;
	}

	public void setPriseLoveAverage(String priseLoveAverage) {
		this.priseLoveAverage = priseLoveAverage;
	}

	public String getPeriodDateFrom() {
		return periodDateFrom;
	}

	public void setPeriodDateFrom(String periodDateFrom) {
		this.periodDateFrom = periodDateFrom;
	}

	public String getPeriodDateTo() {
		return periodDateTo;
	}

	public void setPeriodDateTo(String periodDateTo) {
		this.periodDateTo = periodDateTo;
	}

	public String getDes() {
		return des;
	}

	public void setDes(String des) {
		this.des = des;
	}

	public List<AssessmentBo> getAsList() {
		return AsList;
	}

	public void setAsList(List<AssessmentBo> asList) {
		AsList = asList;
	} 
	
}
