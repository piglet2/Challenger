package com.envision.envservice.common.enums;

/**
 * 上下属关系
 * 
 * @ClassName LevelRelation
 * @author guowei.wang
 * @date 2015-11-5
 */
public enum LevelRelation {

	/**
	 * 上级
	 */
	HL(1),
	
	/**
	 * 平级
	 */
	EL(2),
	
	/**
	 * 下级
	 */
	LL(3),
	
	/**
	 * 无上下级关系
	 */
	NO(4);
	
	private int value;

	private LevelRelation(int value) {
		this.value = value;
	}

	public int getValue() {
		return value;
	}
}
