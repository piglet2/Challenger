<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"   
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.envision.envservice.dao.RuleItemDao">

	<select id="queryByValidator" parameterType="String" resultType="com.envision.envservice.entity.dto.RuleItem" >
		SELECT * 
		FROM rule_item 
		WHERE validator = #{validator}
	</select>

</mapper>